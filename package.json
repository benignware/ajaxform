{
  "name": "ajaxform",
  "version": "0.0.1",
  "description": "Simple ajaxform automation",
  "main": "ajaxform.js",
  "scripts": {
    "start": "npm run watch & npm run start-server",
    "start-server": "./node_modules/http-server/bin/http-server . -o",
