{
  "name": "remoteform",
  "version": "0.0.3",
  "description": "Ajaxify forms easily",
  "main": "remoteform.js",
  "scripts": {
    "start": "npm run watch & npm run start-server",
    "start-server": "./node_modules/http-dev-server/bin/http-dev-server -f httpconfig.js",
    "start-parcel": "./node_modules/parcel-bundler/bin/cli.js index.html",
    "watch": "./node_modules/parcel-bundler/bin/cli.js watch ./remoteform.js",
    "build": "./node_modules/parcel-bundler/bin/cli.js build ./remoteform.js",
    "test": "./node_modules/mocha/bin/mocha remoteform.spec.js --require babel-core/register",
    "lint": "./node_modules/.bin/eslint remoteform.js"
  },
  "keywords": [
    "ajax",
    "form"
  ],
  "author": "Rafael Nowrotek",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-latest": "^6.24.1",
    "eslint": "^4.16.0",
    "fetch-mock": "^6.0.0-beta.9",
    "formidable": "^1.1.1",
    "http-dev-server": "^0.2.3",
    "http-server": "^0.11.1",
    "install": "^0.10.2",
    "isomorphic-fetch": "^2.2.1",
    "jsdom": "^11.6.0",
    "mocha": "^5.0.0",
    "npm": "^5.6.0",
    "nyc": "^11.4.1",
    "parcel-bundler": "^1.5.0",
    "qs": "^6.5.1",
    "querystring": "^0.2.0",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "get-form-data": "^2.0.0",
    "unique-selector": "^0.3.6"
  }
}
