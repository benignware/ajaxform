    "eslint": "^4.16.0",
    "fetch-mock": "^6.0.0-beta.9",
    "http-server": "^0.11.1",
    "install": "^0.10.2",
    "isomorphic-fetch": "^2.2.1",
    "jsdom": "^11.6.0",
    "mocha": "^5.0.0",
    "node-fetch-polyfill": "^2.0.6",
    "npm": "^5.6.0",
    "nyc": "^11.4.1",
    "parcel-bundler": "^1.5.0",
    "webpack": "^3.10.0"
